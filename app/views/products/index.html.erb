<p style="color: green"><%= notice %></p>

<h1>Products</h1>

<div class="products my-products">
  <% @products.each do |product| %>
    <%= link_to product, class: 'product-card' do %>
      <%= render 'product_card', product: product %>
    <% end %>
  <% end %>
</div>

<%= link_to "New product", new_product_path %>
